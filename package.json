{
  "name": "sourcery-web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build --spa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "nodemon --watch server --exec \"nuxt\"",
    "dev-local": "EMULATOR=1 nodemon --watch server --exec \"nuxt\"",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "heroku-postbuild": "nuxt build --universal",
    "tests": "nightwatch --headless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "post-checkout": "yarn",
      "post-merge": "git remote prune origin"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/toast": "^3.2.1",
    "@nuxtjs/vuetify": "^0.5.6",
    "accounting-js": "^1.1.1",
    "algoliasearch": "^4.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "firebase": "^7.6.1",
    "firebase-tools": "^6",
    "geolib": "^3.0.3",
    "global": "^4.3.2",
    "instantsearch.css": "^7.4.2",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "lodash.throttle": "^4.1.1",
    "material-design-icons-iconfont": "^5.0.1",
    "md5": "^2.2.1",
    "nuxt": "2",
    "paymentfont": "^1.2.5",
    "search-insights": "^1.4.0",
    "vue-cookie-law": "^1.11.0",
    "vue-instantsearch": "^2.7.0",
    "vuetify-loader": "^1.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chromedriver": "^83.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-vue": "^5.2.2",
    "faker": "^4.1.0",
    "husky": "^2.4.0",
    "nightwatch": "^1.3.6",
    "nodemon": "^1.19.1",
    "sass-loader": "^7.0.3",
    "sourcery-env": "git+ssh://git@github.com/GreenhouseStudios/sourcery-env.git#v1.1.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.7.0",
    "webpack": "4.33.0"
  },
  "description": "Frontend application for Tube",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "contributors": [
    "Brian Daley <brian@uconn.edu>"
  ]
}
